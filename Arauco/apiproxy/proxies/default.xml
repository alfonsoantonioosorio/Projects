<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
  <PreFlow name="PreFlow">
    <Request/>
    <Response/>
  </PreFlow>
  <Flows>
    <Flow name="post /sap/bc/srt/scs/sap/zsi_is_patenteentrega/{sap-client}">
      <Description>Retrieve list of historical states for devices belonging to organization</Description>
      <Request/>
      <Response/>
      <Condition>(proxy.pathsuffix MatchesPath "/sap/bc/srt/scs/sap/zsi_is_patenteentrega/*") and (request.verb = "POST")</Condition>
    </Flow>
  </Flows>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <HTTPProxyConnection>
    <BasePath>/client-patent-sap/v1</BasePath>
  </HTTPProxyConnection>
  <RouteRule name="default">
    <TargetEndpoint>default</TargetEndpoint>
  </RouteRule>
</ProxyEndpoint>
